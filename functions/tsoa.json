{
    "entryFile": "./src/index.ts",
    "noImplicitAdditionalProperties": "silently-remove-extras",
    "swagger": {
        "entryFile": "./src/index.ts",
        "outputDirectory": "./dist",
        "host": "localhost:3000",
        "basePath": "/v1",
        "securityDefinitions": {
            "api_key": {
                "type": "basic"
            }
        },
        "yaml": true,
        "specVersion": 3
    },
    "routes": {
        "entryFile": "./src/index.ts",
        "basePath": "/v1",
        "routesDir": "./src/routes",
        "authenticationModule": "./src/auth/authentication.ts",
        "middleware": "express"
    }
}